{
  "name": "lara-dev",
  "type": "module",
  "version": "0.0.2",
  "description": "Lara Dev",
  "main": "index.js",
  "keywords": [
    "laratranslate",
    "lara",
    "localization",
    "translation",
    "ai",
    "translate",
    "translated"
  ],
  "author": "CristianBelli01K",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "bin": {
    "lara-dev": "./build/index.js"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@inquirer/core": "^10.2.2",
    "@inquirer/prompts": "^7.8.4",
    "@translated/lara": "^1.6.5",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "flat": "^6.0.1",
    "gettext-parser": "^8.0.0",
    "glob": "^11.0.3",
    "ora": "^8.2.0",
    "picomatch": "^4.0.3",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "string-width": "^8.1.0",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.1"
  },
  "imports": {
    "#utils/*": "./build/utils/*",
    "#modules/*": "./build/modules/*",
    "#cli/*": "./build/cli/*",
    "#messages/*": "./build/messages/*"
  },
  "scripts": {
    "build": "rm -rf build && npx tsc",
    "start": "rm -rf build && npx tsc && node build/index.js",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/gettext-parser": "^8.0.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.3.0",
    "@types/picomatch": "^4.0.2",
    "@vitest/coverage-v8": "4.0.15",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^4.0.15"
  }
}
